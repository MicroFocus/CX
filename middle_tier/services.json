      "data": {
        "response_function_name": "OSPProxy.check",
        "function_source_uri": "plugins.osp.osp_security_proxy",
        "username": "cx",
        "password": "secret",
        "app": "idm",
        "target_url": "http://192.168.0.76:8080"
      }
    }

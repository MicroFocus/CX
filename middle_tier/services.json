        },
        {
          "response_function_name": "SSPRResource.multiple_random_passwords",
          "function_source_uri": "plugins.sspr.password",
          "path": "common_randompassword",
          "method": "GET"
        }
      ],
      "proxy": {
        "listen_path": "/registration/",
